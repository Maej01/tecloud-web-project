<!-- checking for the current page the user is viewing-->
<% if(pageID == 'home') { %> 
    <script>
        $('#homeLink').addClass('li_border_style');
    </script>
<% } else if(pageID == 'about') { %>
    <script>
        $('#aboutLink').addClass('li_border_style');
    </script>
<% } else if(pageID == 'services') { %>
    <script>
        $('#servicesLink').addClass('li_border_style');
    </script>
<% } else if(pageID == 'ourTeamPage'){ %>
    <script>
        $('#ourTeamLink').addClass('li_border_style');
    </script>
<% } else { %>
    <script>
        $('#contactLink').addClass('li_border_style');
    </script>
<% } %>




